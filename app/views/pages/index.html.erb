<div style="width: 625px;" class="green_section">
  <div class="section_header">
  </div>
  <div style="font-family: 'Droid Sans', arial, serif;" class="section_content">
    <h2 style="font-size: 14pt; color: #fff; margin: 0; padding: 0;">
      <%= link_to_unless_current "Latest", root_path do link_to "Latest", root_path, :class => "current" end %>&nbsp;|&nbsp;<%= link_to_unless_current "Awesomest", awesomest_path do link_to "Awesomest", awesomest_path, :class => "current" end %>
    </h2>
    <%= render :partial => "shared/geaket", :collection => @geakets, :locals => { :margin_top => 10 } %>
    <div style="clear: both; font-weight: bold; text-align: right; padding-right: 10px;">
      &nbsp;<%= will_paginate(@geakets) %>
    </div>
  </div>
  <div class="section_footer">
  </div>
</div>
